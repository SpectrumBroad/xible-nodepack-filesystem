  "inputs": {
    "files": {
      "type": "filesystem.file"
    }
  },
  "outputs": {
    "trigger": {
      "type": "trigger"
    },
    "file": {
      "type": "filesystem.file",
      "description": "The file which got created."
    }
  }
}
